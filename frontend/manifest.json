{
    "name": "Wingman",
    "description": "Your personal wingman!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_icon": "icon.jpg",
      "default_title" : "CLICK ME",
      "default_popup": "home.html"
    },
    "background": {
    "service_worker": "background.js",
    "type": "module"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "history",
      "storage"
    ],
    "externally_connectable": {
    "host_permissions": [
    ]
  }
}