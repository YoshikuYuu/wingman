{
    "name": "Wingman",
    "description": "Your personal wingman!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_icon": "icon.jpg",
      "default_title" : "CLICK ME",
      "default_popup": "home.html"
    },
    "web_accessible_resources": [{
      "resources": ["home.html", "iframe.html"],
      "matches": ["<all_urls>"]
    }],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "host_permissions": [
      "https://discord.com/*"
    ],
    "permissions": [
      "scripting",
      "tabs",
      "activeTab"
    ]
}